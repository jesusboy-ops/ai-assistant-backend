{
  "name": "ai-assistant-backend",
  "version": "1.0.0",
  "description": "Smart AI Personal Assistant Backend - Production Ready",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "generate-vapid": "node src/utils/generateVapid.js",
    "test-endpoints": "node test-endpoints.js",
    "test-life-admin": "node test-life-admin.js",
    "setup-production": "node setup-production.js",
    "health-check": "curl -f http://localhost:5000/health || exit 1",
    "install-deps": "npm install",
    "setup": "node setup.js",
    "fix": "node quick-fix.js",
    "docker:build": "docker build -t ai-assistant-backend .",
    "docker:run": "docker run -p 5000:5000 --env-file .env ai-assistant-backend",
    "docker:compose": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down",
    "backup:db": "node scripts/backup-database.js",
    "restore:db": "node scripts/restore-database.js",
    "migrate:db": "node scripts/migrate-database.js",
    "clean:logs": "node scripts/clean-logs.js",
    "security:audit": "npm audit && npm audit fix",
    "update:deps": "npm update && npm audit fix"
  },
  "keywords": [
    "ai",
    "assistant",
    "jarvis",
    "gpt",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "@supabase/supabase-js": "^2.39.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "cloudinary": "^1.41.0",
    "compression": "^1.8.1",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.5",
    "googleapis": "^128.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
